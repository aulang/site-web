{
  "compilerOptions": {
    "types": ["node"],
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "jsx": "preserve",
  "include": ["src/**/*"],
  "exclude": ["dist", "node_modules"],
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  }
}
